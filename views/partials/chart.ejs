
<% if(chartid && chart){ %>

<script>
fromServer = {};
fromServer.chartid = '<%= chartid %>';
fromServer.data = <%- chart %>;
</script>

<% } %>

<div class="row">
	<div class="col s12 m12 l12" id="toolbar">
		<div class="waves-effect grey lighten-1 btn-flat black-text" id="addNap">Nap <i class="fa fa-plus-square-o"></i></div>
		<div class="waves-effect grey lighten-1 btn-flat black-text" id="addCore">Core <i class="fa fa-plus-square-o"></i></div>
		<div class="waves-effect waves-light grey darken-3 btn-flat white-text" id="addBusy">Busy <i class="fa fa-plus-square-o"></i></div>
		<div id="removeAll" class="btn-flat waves-effect">Remove all <i class="fa fa-square-o"></i></div>


	</div>

	<div class="col s12 m7 l7">
		<canvas width="330" height="330" id="canvas">
		</canvas>
		<!--<canvas width="330" height="330" id="canvas">
	</canvas>-->
	</div>

	<div class="col s12 m5 l5">
		<div id="formInputContainer"></div>

		<div class="card stat-block left">
			<div class="card-headline">Sleep time</div>
			<div class="inner-card">
				<span class="stat-time sleep"></span>
				<div class="stat-desc sleep"></div>
			</div>
		</div>

		<!--<div class="card stat-block left">
			<div class="card-headline">Free time</div>
			<div class="inner-card">
				<span class="stat-time free"></span>
				<div class="stat-desc free"></div>
			</div>
		</div>-->

		<div class="card stat-block left">
			<div class="card-headline">Schedule:</div>
			<div class="inner-card">
				<textarea  id="textSchedule" rows="4"  readonly></textarea>
			</div>
		</div>

	</div>
</div>
